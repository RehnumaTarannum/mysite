            	<div class="col-md-8">
                	<form name="sentMessage" id="contactForm" novalidate>
                    	<div class="control-group form-group">
                        	<div class="controls">
                            	<label style="color:#768586">Full Name:</label>
                            	<input type="text" class="form-control" id="name" required data-validation-required-message="Please enter your name." name="name" placeholder="First and Last Name">
                        	</div> 

                        	<div class="control-group form-group">
                        		<div class="controls">
                            		<label style="color:#768586">Phone Number:</label>
                            		<input type="tel" class="form-control" id="phone" name="contact" placeholder="Phone Number (Optional)">
                        		</div>
                    		</div> 
                    		<div class="control-group form-group">
                        		<div class="controls">
                            		<label style="color:#768586">Email Address:</label>
                            		<input type="email" class="form-control" id="email" required data-validation-required-message="Please enter your email address." name="_replyto" placeholder="E-mail Address">
                        		</div>
                    		</div> 

                    		<div class="control-group form-group">
                        		<div class="controls">
                            		<label style="color:#768586">Message:</label>
                            		<textarea rows="10" cols="100" class="form-control" id="message" required data-validation-required-message="Please enter your message" maxlength="999" style="resize:none" name="message" placeholder="Message not more than 1000 words"></textarea>
                        		</div>
                    		</div>


                    		<div id="success"></div>
                    	
                    		<button type="submit" class="btn btn-success">
                            	<span class='glyphicon glyphicon-send'></span>

                            	&nbsp&nbspSend Message
                            
                        	</button>

                  		</div>
                  	</form>
                </div>
            </div>
